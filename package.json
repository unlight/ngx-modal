{
  "name": "ngx-modal2",
  "description": "Open modal window for your Angular X application",
  "version": "4.7.1",
  "scripts": {
    "dev": "webpack-dev-server --hot --progress --colors",
    "karma": "karma start",
    "test:r": "npm run karma -- --single-run",
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:w": "npm run karma",
    "build": "sh Taskfile build",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "eslint src --ext ts",
    "eslint:w": "sh Taskfile eslint_watch",
    "eslint:fix": "eslint src --ext ts --fix",
    "tslint:fix": "tslint -p tsconfig.json --fix",
    "lint:w": "run-p tsclint:w eslint:w",
    "prepublishOnly": "sed -i -e 's/devDependencies/_devDependencies/g' package.json"
  },
  "keywords": [
    "modal",
    "ngx",
    "angular2"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/ngx-modal.git"
  },
  "homepage": "https://github.com/unlight/ngx-modal",
  "peerDependencies": {
    "rxjs": "5.X",
    "@angular/core": ">=2.4 <6",
    "@angular/common": ">=2.4 <6",
    "@angular/router": ">=2.4 <6"
  },
  "devDependencies": {
    "@angular/common": "5.X",
    "@angular/compiler": "5.X",
    "@angular/compiler-cli": "5.X",
    "@angular/core": "5.X",
    "@angular/platform-browser": "5.X",
    "@angular/platform-browser-dynamic": "5.X",
    "@angular/router": "5.X",
    "@types/core-js": "2.5.0",
    "@types/eslint": "4.16.6",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jasmine": "3.3.12",
    "@types/karma": "3.0.2",
    "@types/karma-jasmine": "0.0.31",
    "@types/karma-webpack": "2.0.6",
    "@types/node": "11.13.2",
    "@types/webpack": "4.4.27",
    "@types/webpack-dev-server": "3.1.5",
    "@typescript-eslint/eslint-plugin-tslint": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "angular-router-loader": "0.8.5",
    "awesome-typescript-loader": "5.2.1",
    "codelyzer": "5.0.0",
    "core-js": "2.6.5",
    "css-loader": "2.1.1",
    "css-to-string-loader": "0.1.3",
    "ejs-loader": "0.3.3",
    "eslint": "5.16.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-perf-standard": "1.0.3",
    "eslint-plugin-unicorn": "8.0.1",
    "eslint-plugin-wix-editor": "2.0.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "inotifywait-win32": "1.0.0",
    "jasmine-core": "3.4.0",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "ng-packagr": "5.0.1",
    "raw-loader": "2.0.0",
