{
  "name": "ngx-modal2",
  "description": "Open modal window for your Angular X application",
  "version": "0.0.0-dev",
  "scripts": {
    "dev": "webpack-dev-server --hot --progress --colors",
    "karma": "karma start",
    "test:r": "npm run karma -- --single-run",
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:w": "npm run karma",
    "build": "sh Taskfile build",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "eslint src --ext ts",
    "eslint:w": "sh Taskfile eslint_watch",
    "eslint:fix": "eslint src --ext ts --fix",
    "tslint:fix": "tslint -p tsconfig.json --fix",
    "lint:w": "run-p tsclint:w eslint:w",
    "prepublishOnly": "sed -i -e 's/devDependencies/_devDependencies/g' package.json",
    "prepush": "npm run test && npm run build"
  },
  "keywords": [
    "modal",
    "ngx",
    "angular2"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/ngx-modal.git"
  },
  "homepage": "https://github.com/unlight/ngx-modal",
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "focus-trap": "5",
    "rxjs": "5.X",
    "@angular/core": ">=2.4 3 4 5",
    "@angular/common": ">=2.4 3 4 5",
    "@angular/router": ">=2.4 3 4 5"
  },
  "devDependencies": {
    "@angular/common": "5.X",
    "@angular/compiler": "5.X",
    "@angular/compiler-cli": "5.X",
    "@angular/core": "5.X",
    "@angular/platform-browser": "5.X",
    "@angular/platform-browser-dynamic": "5.X",
    "@angular/router": "5.X",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/commit-analyzer": "7.0.0-beta.2",
    "@semantic-release/git": "7.1.0-beta.3",
    "@semantic-release/github": "5.4.0-beta.1",
    "@semantic-release/npm": "5.2.0-beta.6",
