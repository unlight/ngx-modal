{
  "name": "ngx-modal2",
  "description": "Open modal window for your Angular X application",
  "version": "4.8.0",
  "scripts": {
    "clean": "rimraf package",
    "start": "webpack-dev-server --hot --progress --colors",
    "start:aot": "npm run start -- --env.aot",
    "gulp": "gulp",
    "karma": "karma start",
    "t": "npm run karma -- --single-run",
    "test": "npm run lint && npm run t",
    "test:w": "npm run karma",
    "build": "ng-packagr -p ng-package.json",
    "postbuild": "npm run gulp -- postbuild",
    "tsc": "tsc",
    "lint": "npm run eslint && npm run tsc -- --noEmit --noUnusedLocals",
    "eslint": "eslint src --ext ts",
    "eslint:fix": "eslint src --ext ts --fix",
    "np": "cd package && npm publish",
    "release": "npm run build && npm run np"
  },
  "keywords": [
    "modal",
    "ngx",
    "angular2"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "typings": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/ngx-modal.git"
  },
  "homepage": "https://github.com/unlight/ngx-modal",
  "devDependencies": {
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/compiler-cli": "5.0.1",
    "@angular/core": "5.0.1",
    "@angular/platform-browser": "5.0.1",
    "@angular/platform-browser-dynamic": "5.0.1",
    "@angular/router": "5.0.1",
    "@ngtools/webpack": "1.8.0",
    "@types/del": "3.0.1",
    "@types/jasmine": "2.8.7",
    "@types/node": "8.0.53",
    "angular-router-loader": "0.7.0",
    "awesome-typescript-loader": "3.3.0",
    "codelyzer": "4.3.0",
    "core-js": "2.5.6",
    "css-loader": "0.28.7",
    "css-to-string-loader": "0.1.3",
    "del": "3.0.0",
    "ejs-loader": "0.3.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-perf-standard": "1.0.3",
    "eslint-plugin-tslint": "2.1.0",
    "eslint-plugin-typescript": "0.8.0",
    "eslint-plugin-unicorn": "3.0.0",
    "eslint-plugin-wix-editor": "1.1.1",
    "file-loader": "1.1.11",
    "gulp": "3.9.1",
    "gulp-ignore": "2.0.2",
    "gulp-load-plugins": "1.5.0",
    "html-webpack-plugin": "2.30.1",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.6",
    "ng-packagr": "1.6.0",
    "puppeteer": "1.7.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "rxjs": "5.5.2",
    "style-loader": "0.19.0",
    "ts-node": "4.1.0",
    "tslib": "1.9.1",
    "tslint": "5.10.0",
    "typescript": "2.8.3",
    "typescript-eslint-parser": "15.0.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "zone.js": "0.8.18"
  }
}
